<%= link_to 'Wikis#Index', wikis_path, class: 'btn btn-primary' %><br/>
<h1><%= @wiki.title %></h1>
<small>
  <%= link_to 'Wikis#Edit', edit_wiki_path, class: 'btn btn-primary' %><br/>
  <%= @wiki.description %><br/>
  <%= p "This wiki has not been tagged" if @tags.count == 0 %>
  <% @tags.each do |x| %>
      <%= link_to x.tag, x %>
  <% end %>
</small>
<hr/>
<p><%= @wiki.body %></p>

<%= link_to "Delete Wiki", @wiki, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this wiki?' } %>
<br />
<br />



