<%= link_to 'Back to Tags#Index', tags_path, class: 'btn btn-primary' %><br/>
<h1><%= @tag.tag %></h1>
<hr/>
<div class="col-md-4">
  <em>Total Wikicount:  <%= @tag.wikis.count %></em>
  <br/>
  <br/>
  <ul>
    <% @wikis.each do |wiki| %>
      <% if wiki.is_wiki_viewable?(current_user) %>
        <li><%= link_to wiki.title, wiki %>
          <small>  Private?  <%= wiki.private %></small>
        </li>
      <% end %>
    <% end %>
  </ul>
</div>